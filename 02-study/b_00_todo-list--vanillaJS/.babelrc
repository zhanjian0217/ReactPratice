{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "targets": {
          "browsers": [
            "> 2%",
            "last 2 versions",
            "IE >= 11"
          ]
        }
      }
    ],
    "@babel/preset-flow",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-refresh/babel"
      ]
    },
    "production": {
      "plugins": [
      ]
    }
  }
}
